{
  "name": "odin-react",
  "version": "1.0.0",
  "description": "Fullstack React Flask Combo",
  "dependencies": {
    "ansi-regex": "^5.0.1",
    "ansi-styles": "^4.3.0",
    "chalk": "^4.1.2",
    "cliui": "^8.0.1",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "emoji-regex": "^8.0.0",
    "escalade": "^3.2.0",
    "get-caller-file": "^2.0.5",
    "has-flag": "^4.0.0",
    "is-fullwidth-code-point": "^3.0.0",
    "lodash": "^4.17.21",
    "require-directory": "^2.1.1",
    "rxjs": "^7.8.2",
    "shell-quote": "^1.8.3",
    "string-width": "^4.2.3",
    "strip-ansi": "^6.0.1",
    "supports-color": "^7.2.0",
    "tree-kill": "^1.2.2",
    "tslib": "^2.8.1",
    "wrap-ansi": "^7.0.0",
    "y18n": "^5.0.8",
    "yargs": "^17.7.2",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "start:frontend": "cd frontend && npx serve dist -l 3000",
    "install:backend:windows": "cd flask_server_api && python -m venv venv && venv\\Scripts\\activate && pip install -r requirements.txt",
    "install:backend:linux": "cd flask_server_api && python3 -m venv venv && source venv/Scripts/activate && pip install -r requirements.txt",
    "start:backend": "waitress-serve --host=localhost --port=5000 flask_server_api.server:app",
    "build:prod:windows": "npm run install:frontend && npm run build:frontend && npm run install:backend:windows",
    "build:prod:linux": "npm run install:frontend && npm run build:frontend && npm run install:backend:linux",
    "start:prod": "concurrently -k \"npm run start:frontend\" \"npm run start:backend\"",
    "complete:prod:windows": "npm run build:prod:windows && \"npm run start:prod\"",
    "complete:prod:linux": "npm run build:prod:linux && \"npm run start:prod\"",
    "start:flask": "flask --app flask_server_api.server run --debug",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyberSNoob/te_react_app.git"
  },
  "keywords": [],
